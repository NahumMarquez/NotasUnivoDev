 @model TeachersModel
<div class="col-xl-12 col-md-12 col-sm-12 mt-3">
    <div class="table-responsive">
        <table class="table table-hover" id="CareersTable">
            <thead class="table-light">
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Is Active</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>@Model.TeacherId</td>
                    <td>@Model.Name @Model.LastName</td>
                    <td>@(Model.IsActive ? "Yes" : "No")</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
